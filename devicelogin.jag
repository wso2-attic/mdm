     <div class="container">
	 <form class="form-signin" action="enroll" method="post" >
		<h3>License Agreement</h3>
		<p><%
			var getResource =  function(name){
				var f = new File(name);
				f.open("r");
				var cont = f.readAll();
				f.close();
				return cont;
			};
			print(getResource('license/license.txt'));
		%></p>
		 <button class="btn btn-lg btn-primary btn-block" >I Accept</a>
		 
	     <input name="username" type="hidden" value="<% print(request.getParameter('username')); %>" />
